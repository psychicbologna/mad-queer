{
  "name": "mad-queer",
  "version": "1.0.0",
  "description": "An accessible, mobile-focused resource sharing app",
  "main": "index.js",
  "scripts": {
    "dev": "npm -s run build && parcel ./src/index.html",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "build": "parcel build",
    "start": "npm -s run build && node dist/backend/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psychicbologna/mad-queer.git"
  },
  "author": "Alex Fukui, Alex Fukui Design LLC",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/psychicbologna/mad-queer/issues"
  },
  "homepage": "https://github.com/psychicbologna/mad-queer#readme",
  "dependencies": {
    "express": "^4.17.3",
    "nth-check": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "targets": {
    "frontend": {
      "source": [
        "src/index.html"
      ],
      "publicUrl": "/frontend"
    },
    "backend": {
      "source": "./server/index.tsx",
      "optimize": false,
      "context": "node",
      "engines": {
        "node": ">=16"
      }
    }
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@types/express": "^4.17.13",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "parcel": "^2.2.1",
    "parcel-transformer-ts-css-modules": "^1.0.14",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-custom-properties": "^12.1.4",
    "postcss-import": "^14.0.2",
    "postcss-modules": "^4.3.1",
    "postcss-url": "^10.1.3",
    "prettier": "^2.5.1",
    "react-helmet-async": "^1.2.3",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  }
}
